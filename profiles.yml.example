# Fichier de configuration des profils dbt
# Ce fichier doit normalement être placé dans ~/.dbt/profiles.yml
# Il contient les informations de connexion à votre base de données

dbt_labs:
  target: dev
  outputs:
    dev:
      # Configuration pour PostgreSQL
      type: postgres
      host: localhost
      port: 5432
      user: votre_user
      password: votre_password
      dbname: votre_database
      schema: dbt_dev
      threads: 4
      keepalives_idle: 0

    prod:
      type: postgres
      host: prod-server.example.com
      port: 5432
      user: votre_user_prod
      password: votre_password_prod
      dbname: votre_database_prod
      schema: dbt_prod
      threads: 8
      keepalives_idle: 0

